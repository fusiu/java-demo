<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fusiu.spring.boot.dao.UserDao">
    <select id="select" resultType="com.fusiu.spring.boot.model.UserDomain">
        SELECT
        *
        FROM
        user
        WHERE
        user_code = #{userCode}
    </select>
</mapper>
